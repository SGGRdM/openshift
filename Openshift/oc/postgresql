oc new-app openshift/postgresql-92-centos7 -e POSTGRESQL_USER=user -e POSTGRESQL_DATABASE=db -e POSTGRESQL_PASSWORD=password
oc set resources dc/postgresql-92-centos7 --limits=memory=80Mi
oc set probe dc/postgresql-92-centos7 --liveness -- echo ok
oc describe svc/postgresql-92-centos7
oc set probe dc/postgresql-92-centos7 --readiness --open-tcp=5432
